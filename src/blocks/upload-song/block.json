{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "songwriter-tools/upload-song",
	"version": "0.1.0",
	"title": "Upload Song",
	"category": "songwriter-tools",
	"icon": "media-audio",
	"description": "Upload music to the WP Media library from the front end of your website",
	"example": {},
	"supports": {
		"interactivity": true,
		"align": false,
		"html": false,
		"color": {
			"background": true,
			"text": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textAlign": false
		}
	},
	"attributes": {
		"headingTag": {
			"type": "string",
			"default": "h3"
		},
		"headingContent": {
			"type": "string",
			"default": "Upload Your Song"
		},
		"allowedMimeTypes": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"default": ["audio/mpeg", "audio/wav"]
		},
		"maxFileSize": {
			"type": "number",
			"default": 15
		}
	},
	"textdomain": "upload-song",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScriptModule": "file:./view.ts"
}
